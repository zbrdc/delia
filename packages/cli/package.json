{
  "name": "@delia/cli",
  "version": "0.1.0",
  "description": "Rich CLI for Delia - local LLM orchestration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "delia-cli": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts",
    "dev": "tsup src/index.tsx --format esm --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "commander": "^12.1.0",
    "eventsource-parser": "^3.0.0",
    "ink": "^5.0.1",
    "ink-spinner": "^5.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.14",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "cli",
    "llm",
    "ai",
    "ollama",
    "local"
  ],
  "license": "GPL-3.0-or-later"
}
