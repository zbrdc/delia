{
  "mcpServers": {
    "shadcn": {
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ]
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "wolfram-alpha": {
      "type": "stdio",
      "command": "node",
      "args": [
        "/home/dan/git/wolfram-mcp/index.js"
      ],
      "env": {
        "WOLFRAM_LLM_APP_ID": "EWR7U42GGT"
      }
    },
    "delia": {
      "type": "stdio",
      "command": "/home/dan/.local/bin/uv",
      "args": [
        "--directory",
        "/home/dan/git/delia",
        "run",
        "delia",
        "serve"
      ],
      "env": {}
    },
    "memory": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {}
    },
    "fetch": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@context7/mcp-server"
      ],
      "env": {}
    },
    "vast-ai": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/home/dan/git/vastai-mcp",
        "vast-mcp-server"
      ],
      "env": {
        "VAST_API_KEY": "${VAST_API_KEY}",
        "SSH_KEY_FILE": "~/.ssh/id_rsa",
        "SSH_KEY_PUBLIC_FILE": "~/.ssh/id_rsa.pub"
      }
    }
  }
}
